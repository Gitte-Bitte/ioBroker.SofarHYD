{ // lets start"i18n": true,
    "type": "tabs",
    "items": {
        "tab_1": {
            "type": "panel",
            "label": "Protokoll",
            "sm": 3,
            "lg": 3,
            "items": {
                "select": {
                    "type": "select",
                    "label": "Bitte Protokoll wählen",
                    "newLine": true,
                    "sm": 3,
                    "lg": 3,
                    "options": [
                        {
                            "label": "TCP",
                            "value": 1
                        },
                        {
                            "label": "Seriell",
                            "value": 2
                        },
                        {
                            "label": "WasWeisIch",
                            "value": 3
                        }
                    ]
                },
                "panel_1": {
                    "type": "panel",
                    "label": "TCP",
                    "hidden": "!(data.select==1)",
                    "newLine": true,
                    "sm": 3,
                    "lg": 3,
                    "items": {
                        "ip": {
                            "type": "ip",
                            "label": "IP-Address",
                            "newLine": true
                        },
                        "port": {
                            "type": "port",
                            "label": "Port auswählen",
                            "newLine": true
                        }
                    }
                },
                "panel_2": {
                    "type": "panel",
                    "hidden": "!(data.select==2)",
                    "newLine": true,
                    "sm": 3,
                    "lg": 3,
                    "items": {
                        "schnittstellen": {
                            "label": "Schnittstelle",
                            "type": "selectSendTo",
                            "command": "nu"
                        },
                        "baudselect": {
                            "type": "select",
                            "label": "Baudrate",
                            "newLine": true,
                            "sm": 3,
                            "lg": 3,
                            "options": [
                                {
                                    "label": "9600",
                                    "value": 9600
                                },
                                {
                                    "label": "11000",
                                    "value": 11000
                                },
                                {
                                    "label": "23000",
                                    "value": 23000
                                }
                            ]
                        },
                        "stopb": {
                            "type": "number",
                            "label": "stopp-bits",
                            "min": 5,
                            "max": 8,
                            "newLine": true,
                            "sm": 3,
                            "lg": 3
                        },
                        "paritätsbit": {
                            "type": "checkbox",
                            "label": "Paritätsbit",
                            "newLine": true,
                            "sm": 3,
                            "lg": 3
                        }
                    }
                },
                "panel_3": {
                    "type": "panel",
                    "label": "TCP",
                    "hidden": "!(data.select==3)",
                    "newLine": true,
                    "sm": 3,
                    "lg": 3,
                    "items": {
                        "selectVal1": {
                            "type": "pattern",
                            "pattern": "Hier steht was weiss ich",
                            "newLine": true
                        },
                        "checkbox2a": {
                            "type": "checkbox",
                            "label": "wasweissich Checkbox ",
                            "tooltip": "enables third checkbox",
                            "newLine": true
                        }
                    }
                },
                "number2": {
                    "type": "number",
                    "label": "Geräte-ID",
                    "min": 1,
                    "max": 128,
                    "newLine": true
                }
            }
        },
        "tab_2": {
            "type": "panel",
            "label": "List:Polling",
            "items": {
                "lintToJson": {
                    "type": "staticLink",
                    "text": "sssssssssssssssssssssssssssssssssssssssssss",
                    "href": "https://github.com/mcm1957/ioBroker.jsonconfig-demo/blob/main/admin/jsonConfig.json5",
                    "icon": "info"
                },
                "table1": {
                    "type": "table",
                    "newLine": true,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "label": "",
                    "showSecondAddAt": 1,
                    "items": [
                        {
                            "type": "checkbox",
                            "attr": "table1Chk",
                            "width": "5% ",
                            "title": "checkbox",
                            "tooltip": "Click on it",
                            "filter": false,
                            "sort": false,
                            "default": true
                        },
                        {
                            "type": "checkbox",
                            "attr": "table1ChkHide",
                            "width": "5% ",
                            "title": "hide",
                            "tooltip": "click to hide fields",
                            "filter": false,
                            "sort": false,
                            "default": false
                        },
                        {
                            "type": "text",
                            "attr": "table1Text1",
                            "width": "8% ",
                            "title": "Text",
                            "tooltip": "enter any text",
                            "filter": false,
                            "sort": false,
                            "default": ""
                        },
                        {
                            "type": "text",
                            "attr": "table1Unique",
                            "width": "8% ",
                            "title": "Unique Text",
                            "tooltip": "enter any text",
                            "filter": false,
                            "sort": false,
                            "default": "",
                            "validator": "const x={};for (const c of globalData.table1) {if (x[c.table1Unique]==='x') return false; x[c.table1Unique]='x';}; return true;",
                            "validatorNoSaveOnError": true
                        },
                        {
                            "type": "text",
                            "attr": "table1Char",
                            "width": "8% ",
                            "title": "Restricted Text",
                            "tooltip": "enter text with 'A-Za-z0-9_' only",
                            "filter": false,
                            "sort": false,
                            "default": "",
                            "validator": "return (data.table1Char==='') || data.table1Char.match(/^[A-Za-z0-9]([A-Za-z0-9-_])*$/);",
                            "validatorNoSaveOnError": true
                        },
                        {
                            "type": "number",
                            "attr": "table1Num",
                            "width": "6% ",
                            "title": "Number",
                            "tooltip": "enter a number",
                            "hidden": "data.table1ChkHide",
                            "filter": false,
                            "sort": false,
                            "min": 0,
                            "step": 0.001,
                            "default": 1
                        }
                    ]
                }
            }
        },
        "tab_3": {
            "type": "panel",
            "label": "List:CronJob",
            "items": {
                "lintToJson": {
                    "type": "staticLink",
                    "text": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa",
                    "href": "https://github.com/mcm1957/ioBroker.jsonconfig-demo/blob/main/admin/jsonConfig.json5",
                    "icon": "info"
                }
            }
        }
    }
}